<?xml version="1.0" encoding="ISO-8859-1"?> 
<!DOCTYPE web-app
   PUBLIC "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
   "http://java.sun.com/dtd/web-app_2_3.dtd">

<web-app>
  <display-name>XML-RPC server</display-name>
  <description>XML-RPC server</description>
  <servlet>
     <servlet-name>xmlrpcserver</servlet-name>
     <servlet-class>com.miquelmillan.autoannotator.server.SemanticServiceProvider</servlet-class>
     <load-on-startup>1</load-on-startup>
  </servlet>

  <servlet-mapping>
    <servlet-name>xmlrpcserver</servlet-name>
    <url-pattern>/*</url-pattern>
  </servlet-mapping>
</web-app>